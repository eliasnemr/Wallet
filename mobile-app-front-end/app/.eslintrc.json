{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "new-cap":"off",
        "valid-jsdoc":"off",
        "require-jsdoc":"off",
        "object-curly-spacing": "off",
        "max-len": 0,
        "padded-blocks": 0
    }
}
